<%= form_with model: @inquiry, url: inquiry_complete_path, local:true do |f| %>
  <h1>お問い合わせ</h1>

  <table>
    <tbody>
      <tr>
        <th>名前</th>
        <td>
          <%= f.hidden_field :name %>
          <%= @inquiry.name %>
        </td>
      </tr>
      <tr>
        <th>メールアドレス</th>
        <td>
          <%= f.hidden_field :email %>
          <%= @inquiry.email %>
        </td>
      </tr>
      <tr>
        <th>お問い合わせ内容</th>
        <td>
          <%= f.hidden_field :message %>
          <%= @inquiry.message %>
        </td>
      </tr>
    </tbody>
  </table>
  <%= f.submit "送信" %>
<% end %>